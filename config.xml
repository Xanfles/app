<?xml version='1.0' encoding='utf-8'?>
<widget id="com.masmargen.cl" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
	<name>SochiORL</name>
	<description>
		SochiORL app
	</description>
	<author email="jgonzalez@masmargen.cl" href="http://masmargen.cl">
		Masmargen Team
	</author>
  <!--<preference name="phonegap-version" value="cli-7.0.1" />
  <icon src="www/res/icon.png" />-->
  <preference name="show-splash-screen-spinner" value="true" /> 
  <preference name="auto-hide-splash-screen" value="true" /> 
  <content src="index.html"/>	
  <preference name="orientation" value="portrait" />
  <allow-navigation href="*" />
  <allow-intent href="*" />
  <access origin="*" />

  <!-- iOS 8.0+ -->
  <!-- iPhone 6 Plus  -->
  <icon gap:platform="ios" src="res/ios/icon-60@3x.png" width="180" height="180" />
  <!-- iOS 7.0+ -->
  <!-- iPhone / iPod Touch  -->
  <icon gap:platform="ios" src="www/res/ios/icon-60.png" width="60" height="60" />
  <icon gap:platform="ios" src="www/res/ios/icon-60@2x.png" width="120" height="120" />
  <!-- iPad -->
  <icon gap:platform="ios" src="www/res/ios/icon-76.png" width="76" height="76" />
  <icon gap:platform="ios" src="www/res/ios/icon-76@2x.png" width="152" height="152" />
  <!-- Spotlight Icon -->
  <icon gap:platform="ios" src="www/res/ios/icon-40.png" width="40" height="40" />
  <icon gap:platform="ios" src="www/res/ios/icon-40@2x.png" width="80" height="80" />
  <!-- iOS 6.1 -->
  <!-- iPhone / iPod Touch -->
  <icon gap:platform="ios" src="www/res/ios/icon.png" width="57" height="57" />
  <icon gap:platform="ios" src="www/res/ios/icon@2x.png" width="114" height="114" />
  <!-- iPad -->
  <icon gap:platform="ios" src="www/res/ios/icon-72.png" width="72" height="72" />
  <icon gap:platform="ios" src="www/res/ios/icon-72@2x.png" width="144" height="144" />
  <!-- iPad Pro -->
  <icon gap:platform="ios" src="www/res/ios/icon-167.png" width="167" height="167" />
  <!-- iPhone Spotlight and Settings Icon -->
  <icon gap:platform="ios" src="www/res/ios/icon-small.png" width="29" height="29" />
  <icon gap:platform="ios" src="www/res/ios/icon-small@2x.png" width="58" height="58" />
  <!-- iPad Spotlight and Settings Icon -->
  <icon gap:platform="ios" src="www/res/ios/icon-50.png" width="50" height="50" />
  <icon gap:platform="ios" src="www/res/ios/icon-50@2x.png" width="100" height="100" />
  <!-- iPad Pro -->
  <icon gap:platform="ios" src="www/res/ios/icon-83.5@2x.png" width="167" height="167" />


  


  <plugin spec="https://github.com/apache/cordova-plugin-legacy-whitelist#122d4b9" />

  <plugin spec="https://github.com/apache/cordova-plugin-inappbrowser#8e3f2f2" />

  <plugin spec="https://github.com/EddyVerbruggen/Toast-PhoneGap-Plugin#4324168" />

  <!-- Ejemplo para instalar plugin desde github <plugin spec="https://github.com/EddyVerbruggen/Toast-PhoneGap-Plugin" source="git"/>-->
</widget>
